
.content {
	display: -webkit-box;
	display: -webkit-flex;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	        flex-direction: column;
	-webkit-box-pack: center;
	-webkit-justify-content: center;
	        justify-content: center;
	padding: 20px;
	box-sizing: border-box;
}
.logo{
	width: 36px;
	height: 36px;
	padding: 100rpx 0 20rpx 0;
	background: url(data:image/png;base64,) no-repeat 100%;
}
.title{
	font-size: 50rpx;
	font-weight: 700;
	padding-bottom: 100rpx;
}
.login{
	width: 155rpx;
	padding: 0 0 5rpx 0;
	border-bottom: 3px solid #007AFF;
}
.uni-input{
	border-bottom: 1px solid #DCDEE0;
	padding: 20rpx 0;
	margin: 40rpx 0 ;
}
.submit{
	margin-top: 80rpx;
	background-color: #007AFF;
	color: #FFFFFF;
}
.apply{
	margin-top: 80rpx;
	text-align: center;
}
.remarks{
	display: block;
	text-align: center;
	/* margin: 0 30rpx; */
	font-size: 14px;
	color: orange;
}

